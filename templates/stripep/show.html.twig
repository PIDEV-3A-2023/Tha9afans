{% extends 'base.html.twig' %}

{% block title %}Stripep{% endblock %}

{% block body %}
    <h1>Stripep</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ stripep.id }}</td>
            </tr>
            <tr>
                <th>Card_number</th>
                <td>{{ stripep.cardNumber }}</td>
            </tr>
            <tr>
                <th>Card_holder</th>
                <td>{{ stripep.cardHolder }}</td>
            </tr>
            <tr>
                <th>Expiration_month</th>
                <td>{{ stripep.expirationMonth }}</td>
            </tr>
            <tr>
                <th>Expiration_year</th>
                <td>{{ stripep.expirationYear }}</td>
            </tr>
            <tr>
                <th>Cvv</th>
                <td>{{ stripep.cvv }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_stripep_index') }}">back to list</a>

    <a href="{{ path('app_stripep_edit', {'id': stripep.id}) }}">edit</a>

    {{ include('stripep/_delete_form.html.twig') }}
{% endblock %}
